@model IEnumerable<ImprovementProjectWebApp.Models.MealPlan>
@{
    ViewData["Title"] = "MealPlanList";
    Layout = "~/Views/Shared/_CustomerLayout.cshtml";
}
@if (Model.Count() == 0)
{
    <h2>您的饮食计划还没有完成，请您耐心等待</h2>

}
else
{
    <div class="list-group-item col-lg-12">

        @{
            
            int count = 1;
            foreach (var item in Model)
            {
                <a class="list-group-item col-lg-12" href="@item.URL" target="_blank">Week @count</a>
                count++;
            }

        }

    </div>
    <div class="clearfix"></div>
    <br />
}

<a class="btn btn-default col-lg-2 col-lg-offset-10 col-md-2 col-md-offset-10 col-sm-12 col-xs-12" asp-action="Index" asp-controller="Customer">返回</a>


