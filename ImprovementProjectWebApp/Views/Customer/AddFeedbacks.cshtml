
@{
    ViewData["Title"] = "AddFeedbacks";
    Layout = "~/Views/Shared/_CustomerLayout.cshtml";
}

<form asp-controller="Customer" asp-action="AddFeedbacks">
    <div class="row">

        <div class="col-lg-12">
            <h3 >问题：</h3>

            <textarea name="Question" class="form-group col-lg-12"  rows="5" >

            </textarea>
        </div>

        <div class="col-lg-12">
            <br />
            <input type="submit" value="提交" class="btn btn-primary col-lg-2 col-lg-offset-10 " />
            <a asp-action="FeedbacksList" asp-controller="Customer" class="btn btn-default col-lg-2 col-lg-offset-10 ">返回</a>
        </div>
    </div>
</form>

